    <style>
.hidden{
  display: none;
}
</style>
    <script type="text/javascript">
  var allowedCat = [];
  var defaultCat = [];
  var userId = networkoperatoruserId;
  var url = new URL(window.location);
  var brandId = url.searchParams.get("brandId")??"";
  var defaultCollection = 'entrepreneurs'
  var currentCollection = url.searchParams.get("collection")??defaultCollection;
  var platformId = networkoperatorId;
  var currency = {"USD":"$", "INR":"₹","GBP":"£"};
  var stDomain = "https://app.shoptype.com";
  var apiDomain = "https://backend.shoptype.com";
  var networkDomain = networksiteUrl;

  defer(setupShop);
  function defer(method) {
    if (window.jQuery) {
        method();
    } else {
        setTimeout(function() { defer(method) }, 100);
    }
  }
  function setupShop(){
    $(document).ready(function() {
      $('.static-container').removeClass('static-container');
      $.get(networkDomain+"/api/v1/collections/markets-page",function( data ) {
        defaultCat = data['summary'].split(",").map(function(item) {
                                                        return item.trim();
                                                    });
        allowedCat = defaultCat;
      });
      loadStories(currentCollection);
    });
  }
  function loadStories(collectionName){
    $.get(networkDomain+"/api/v1/collections/{collection}".replace('{collection}',collectionName),function( data ) {
      if(collectionName!=defaultCollection && data.items.length == 0){
        loadStories(defaultCollection);
        return;
      }
      var storieItems = $('.stroy-links');
      for (var i = 0; i < 6 && i <storieItems.length && i <data.items.length; i++) {
        if(data.items[i].type== "story"){
          addStoryData($(storieItems[i]), data.items[i]);
        }
      };
      $.get(apiDomain + "/platforms/{platformId}/vendors".replace("{platformId}",platformId),function( data ) {
        var brandItems = $(".brand-link");
        $(".brand-link").addClass("hidden")
        for (var i = 0; i < brandItems.length && i < data.length; i++) {
          brandItems[i].href = networksiteUrl + "/market/?vendorId="+data[i].id;
          $(brandItems[i]).find(".brand-title").html(data[i].name);
          if(data[i].logo){
            $(brandItems[i]).find(".brand-image").attr("src", data[i].logo);
            $(brandItems[i]).find(".brand-title-block").css("display", "none");
          }
          $(brandItems[i]).removeClass("hidden")
        }
        $('.nav-block').removeClass("hidden");
        $('.brands-top').removeClass("hidden");
      });
    });
  }
  function addStoryData(storyElement,story){
    storyElement.attr("href", story.story.slug);
    storyElement.find(".story-title").html(story.story.headline);
    storyElement.find(".story-image").attr("src", 'https://gumlet.assettype.com/'+story.story['hero-image-s3-key']);
  }
</script>
  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
